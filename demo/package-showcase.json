  "repository": {
    "type": "git",
    "url": "https://github.com/rizal-dev/schiedule-main.git"
  },
  "bugs": {
    "url": "https://github.com/rizal-dev/schiedule-main/issues"
  },
  "homepage": "https://schiedule-app.netlify.app",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
