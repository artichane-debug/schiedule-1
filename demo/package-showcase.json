{
  "name": "schiedule-showcase",
  "version": "2.1.0",
  "description": "ðŸŽ“ Advanced College Schedule Management System with AI-powered features",
  "private": false,
  "type": "module",
  "scripts": {
    "dev": "vite --host 0.0.0.0 --port 8081",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "showcase": "node scripts/dev-showcase.js",
    "fake-build": "bash scripts/fake-build.sh",
    "monitor": "python scripts/dev-monitor.py",
    "deploy": "npm run build && netlify deploy --prod",
    "analyze": "npm run build && npx vite-bundle-analyzer dist/stats.html",
    "clean": "rm -rf dist node_modules/.vite",
    "update-deps": "npx npm-check-updates -u && npm install",
    "security-audit": "npm audit --audit-level moderate",
    "performance": "lighthouse http://localhost:8081 --output html --output-path ./reports/lighthouse.html",
    "docker:build": "docker build -t schiedule:latest .",
    "docker:run": "docker run -p 8081:80 schiedule:latest"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-switch": "^1.0.3",
    "@radix-ui/react-tooltip": "^1.0.7",
    "@radix-ui/react-separator": "^1.0.3",
    "@tanstack/react-query": "^5.8.4",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "lucide-react": "^0.294.0",
    "tailwind-merge": "^2.0.0",
    "tailwindcss-animate": "^1.0.7",
    "framer-motion": "^10.16.5",
    "date-fns": "^2.30.0",
    "react-hook-form": "^7.48.2",
    "@hookform/resolvers": "^3.3.2",
    "zod": "^3.22.4",
    "zustand": "^4.4.7",
    "react-router-dom": "^6.18.0",
    "react-hot-toast": "^2.4.1"
  },
  "devDependencies": {
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@types/node": "^20.9.0",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "@vitejs/plugin-react": "^4.1.1",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.53.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.4",
    "postcss": "^8.4.31",
    "tailwindcss": "^3.3.5",
    "typescript": "^5.2.2",
    "vite": "^4.5.0",
    "vitest": "^0.34.6",
    "@vitest/ui": "^0.34.6",
    "jsdom": "^22.1.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/jest-dom": "^6.1.4",
    "chalk": "^5.3.0",
    "figlet": "^1.7.0",
    "ora": "^7.0.1",
    "lighthouse": "^11.3.0",
    "vite-bundle-analyzer": "^0.7.0",
    "npm-check-updates": "^16.14.11"
  },
  "keywords": [
    "react",
    "typescript",
    "tailwindcss",
    "vite",
    "schedule",
    "education",
    "college",
    "university",
    "calendar",
    "productivity",
    "student-tools",
    "academic",
    "time-management"
  ],
  "author": {
    "name": "RIZAL",
    "email": "developer@schiedule.app",
    "url": "https://github.com/rizal-dev"
  },
